{
    "ApiAuth": {
        "Audience": "smallwarehouse",
        "Issuer": "https://localhost:4432",
        "SecretKey": "smallwarehourse123456789"
    },
    "Serilog": {
        "MinimumLevel": {
            "Default": "Debug",
            "Override": {
                "Microsoft": "Warning",
                "System": "Warning"
            }
        },
        "WriteTo": [
            {
                "Name": "File",
                "Args": {
                    "path": ".\\LOGs\\log_webapi.txt",
                    "rollingInterval": "Day",
                    "rollOnFileSizeLimit": "true",
                    "filesSizeLimitBytes": "10485760"
                }
            }
        ]
    },
    "ConnectionStrings": {
        "ConnectionString": "Data Source=.;Initial Catalog=SMALLWAREHOUSE;Integrated Security=True"
    },
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft": "Warning",
            "Microsoft.Hosting.Lifetime": "Information"
        }
    },
    "AllowedHosts": "*"
}
